<Page
    x:Class="HENG.UWP.Views.HomePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:HENG.UWP.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ucs="using:HENG.UWP.UserControls"
    NavigationCacheMode="Enabled"
    mc:Ignorable="d">

    <Grid>
        <ucs:PhotoGridView ViewModel="{x:Bind ViewModel.PhotoGirdViewModel, Mode=OneWay}" />
        <ucs:PhotoItemView ViewModel="{x:Bind ViewModel.PhotoItemViewModel, Mode=OneWay}" />
        <ucs:PhotoSearchView ViewModel="{x:Bind ViewModel.PhotoSearchViewModel, Mode=OneWay}" />
    </Grid>
</Page>
