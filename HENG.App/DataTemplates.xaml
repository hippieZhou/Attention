<ResourceDictionary
    x:Class="HENG.App.DataTemplates"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:HENG.App"
    xmlns:view="using:HENG.App.Views"
    xmlns:vm="using:HENG.App.ViewModels">
    <DataTemplate x:Key="HomeViewModel" x:DataType="vm:HomeViewModel">
        <view:HomeView />
    </DataTemplate>
    <DataTemplate x:Key="LocalViewModel" x:DataType="vm:LocalViewModel">
        <view:LocalView />
    </DataTemplate>
</ResourceDictionary>
