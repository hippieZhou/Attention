<prismUnity:PrismUnityApplication
    x:Class="Attention.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:Attention.App.Converters"
    xmlns:local="using:Attention.App"
    xmlns:prismUnity="using:Prism.Unity.Windows">
    <prismUnity:PrismUnityApplication.Resources>
        <ResourceDictionary>
            <converter:ImageSourceNullValueConverter x:Key="ImageSourceNullValueConverter" />

            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!--  Other merged dictionaries here  -->
                <ResourceDictionary Source="/Themes/GridViewItem.xaml" />
                <ResourceDictionary Source="/Themes/ScrollViewer.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </prismUnity:PrismUnityApplication.Resources>
</prismUnity:PrismUnityApplication>
